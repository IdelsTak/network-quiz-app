<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="800.0" spacing="12.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="connectionStatusLabel" alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="30.0" style="-fx-font-weight: bold; -fx-border-color: gray; -fx-background-color: white;" text="&lt;Not Connected to Server&gt;" />
      <HBox maxHeight="1.7976931348623157E308" spacing="24.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="0.0" spacing="6.0" HBox.hgrow="ALWAYS">
               <children>
                  <TableView fx:id="questionsTable" maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                    <columns>
                      <TableColumn fx:id="questionNumberColumn" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="#" />
                      <TableColumn fx:id="topicColumn" prefWidth="75.0" text="Topic" />
                        <TableColumn fx:id="subTopicColumn" prefWidth="75.0" text="Sub-topic" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#loadQuestionsFile" text="Load questions file" />
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <VBox VBox.vgrow="ALWAYS">
                     <children>
                        <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Topic" />
                              <TextField fx:id="topicTextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Sub-topic" />
                              <TextField fx:id="subTopicTextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Question" />
                              <TextField fx:id="questionTextField" editable="false" />
                           </children>
                        </VBox>
                        <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                           <children>
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Option 1">
                                 <padding>
                                    <Insets top="24.0" />
                                 </padding>
                              </Label>
                              <TextField fx:id="optionATextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Option 2" />
                              <TextField fx:id="optionBTextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Option 3" />
                              <TextField fx:id="optionCTextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Option 4" />
                              <TextField fx:id="optionDTextField" editable="false" />
                              <Label maxWidth="1.7976931348623157E308" style="-fx-font-weight: bold;" text="Option 5" />
                              <TextField fx:id="optionETextField" editable="false" />
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <Label maxWidth="1.7976931348623157E308" text="Answer (1, 2, 3, 4, or 5)">
                     <padding>
                        <Insets top="12.0" />
                     </padding>
                  </Label>
                  <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="12.0">
                     <children>
                        <TextField fx:id="answerTextField" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <Button fx:id="sendButton" defaultButton="true" disable="true" mnemonicParsing="false" text="Send" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <VBox maxHeight="1.7976931348623157E308" spacing="12.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Linked List" />
                  <TextArea />
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Binary Tree" />
                  <TextArea />
                  <HBox spacing="24.0">
                     <children>
                        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Pre-order" HBox.hgrow="ALWAYS" />
                        <Button layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="In-order" HBox.hgrow="ALWAYS" />
                        <Button layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Post-order" HBox.hgrow="ALWAYS" />
                        <Button layoutX="70.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Tree View" HBox.hgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets top="12.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
   </padding>
</VBox>
