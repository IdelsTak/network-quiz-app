<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox style="-fx-background-color: #163864;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="company.policy.client.staff.QuestionViewController">
   <children>
      <HBox maxWidth="1.7976931348623157E308" style="-fx-background-color: #2C64B6;">
         <children>
                <Label fx:id="lblPolicyQuestionMainHeader" alignment="CENTER" maxWidth="1.7976931348623157E308" text="%policy_question" textFill="WHITE" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="36.0" />
                    </font>
                </Label>
         </children>
      </HBox>
      <VBox spacing="12.0">
         <children>
            <VBox>
               <children>
                    <Label fx:id="lblYourName" maxWidth="1.7976931348623157E308" text="%label.staff_name" textFill="WHITE">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <TextField fx:id="txtWord11" prefHeight="30.0" />
               </children>
               <padding>
                  <Insets bottom="12.0" />
               </padding>
            </VBox>
            <VBox>
               <children>
                  <VBox spacing="6.0">
                     <children>
                        <VBox>
                           <children>
                                    <Label fx:id="txtTopicClient" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="%label.topic" textFill="WHITE" wrapText="true">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField id="txtTopic" fx:id="txtTopic" prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestionClient" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="%label.question" textFill="WHITE" wrapText="true">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtQuestionText" alignment="TOP_LEFT" editable="false" prefHeight="30.0" />
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="12.0" />
                     </padding>
                  </VBox>
                  <VBox spacing="6.0">
                     <children>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestion1Client" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="1:" textFill="WHITE">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtAnswerA" prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestion2Client" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="2:" textFill="WHITE">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtAnswerB" prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestion3Client" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="3:" textFill="WHITE">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtAnswerC" prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestion4Client" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="4:" textFill="WHITE">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtAnswerD" prefHeight="30.0" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                                    <Label fx:id="txtQuestion5Client" depthTest="DISABLE" maxWidth="1.7976931348623157E308" text="5:" textFill="WHITE">
                                        <font>
                                            <Font size="18.0" />
                                        </font>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </Label>
                                    <TextField fx:id="txtAnswerE" prefHeight="30.0" />
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets left="12.0" right="12.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
            <HBox alignment="CENTER" spacing="12.0">
               <children>
                    <Label fx:id="lblYourAnserClient" maxHeight="1.7976931348623157E308" text="%label.your_answer" textFill="WHITE" wrapText="true">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <TextField fx:id="txtWord1" prefHeight="31.0" prefWidth="50.0" />
               </children>
            </HBox>
            <VBox spacing="12.0">
               <children>
                    <Label fx:id="lblMessage" alignment="TOP_LEFT" prefHeight="28.0" prefWidth="534.0" wrapText="true" />
                  <HBox alignment="CENTER" spacing="12.0">
                     <children>
                          <Button fx:id="btnConnectClientView" mnemonicParsing="false" onAction="#buttonConnectFunction" prefHeight="44.0" prefWidth="158.0" style="-fx-background-color: #2C64B6; -fx-text-fill: white; -fx-font-weight: bold;" text="%button.connect" wrapText="true" />
                          <Button fx:id="btnSendClientView" disable="true" mnemonicParsing="false" prefHeight="44.0" prefWidth="158.0" style="-fx-background-color: #2C64B6; -fx-text-fill: white; -fx-font-weight: bold;" text="%button.submit" wrapText="true" />
                          <Button fx:id="btnExitClientView" mnemonicParsing="false" onAction="#setBtnExitClientViewAction" prefHeight="44.0" prefWidth="158.0" style="-fx-background-color: #2C64B6; -fx-text-fill: white; -fx-font-weight: bold;" text="%button.exit" wrapText="true" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
